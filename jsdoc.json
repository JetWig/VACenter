{
    "source":{
        "include": [],
        "includePattern": ".+\\.js$"
    },
    "plugins": ["plugins/markdown"],
    "templates":{
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "VACenter",
        "systemSummary": "A revolutionary Crew Center",
        "systemColor": "red",
        "footer": "Ya boi",
